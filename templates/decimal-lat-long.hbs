{
    "IANAVersion": "{{version}}",
    "originalFileName": "{{originalFileName}}",
    "numberOfZones": {{numberOfZones}},
    "zones": [
    {{#each zones}}
        {
            "countryCode": "{{countryCode}}",
            "timezone": "{{timezoneName}}",
            "coordinates": {
                "latitude": {{coordinates.latitude.decimal}},
                "longitude": {{coordinates.longitude.decimal}}
            }
        }{{#unless @last}},{{/unless}}
    {{/each}}
    ]
}
